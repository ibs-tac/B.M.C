<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherit_sale_order_form" model="ir.ui.view">
        <field name="name">inherit.sale.order.form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='note_group']" position="after">
                <group>
                    <group>
                        <field name="sales_executive"/>
                    </group>
                    <group>
                        <field name="sales_manager"/>
                    </group>
                </group>

            </xpath>

            <xpath expr="//field[@name='order_line']/tree//field[@name='product_uom_qty']" position="after">
                <field name="pregnancy"/>
                <field name="weight"/>
            </xpath>
        </field>
    </record>
</odoo>
